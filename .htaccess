AddOutputFilterByType DEFLATE text/html text/plain text/xml text/css application/javascript text/javascript

<FilesMatch "\.(php|cgi|pl)$">
ExpiresActive Off
Header set Cache-Control "private, no-cache, no-store, proxy-revalidate, no-transform"
Header set Pragma "no-cache"
</FilesMatch>

<FilesMatch "\.(html|ico|pdf|flv|jpg|jpeg|png|gif|js|css|swf)$">
Header set Cache-Control "max-age=36000, must-revalidate"
</FilesMatch>
